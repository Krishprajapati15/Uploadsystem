<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Index</title>
</head>
<body>
    <h1>File Upload</h1>
    <form action="/upload" method="post" enctype="multipart/form-data">
        <input type="file" name="file" required>
        <button type="submit">Upload</button>
    </form>
    <h2>Uploded Files</h2>
    <ul>
        <% files.forEach(file => { %>
         <li> <%= file.filename%> - <a href="<%= file.path %>" target="_blank">View</a></li>
       <% }); %>
    </ul>
</body>
</html>